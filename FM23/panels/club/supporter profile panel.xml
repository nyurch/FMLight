				<widget class="table" id="fptb" layout="-2, -2, -1" mode="fill_rows, stripe_rows" auto_size_rows="false" fixed_size_rows="true" row_height="26" height="108">
					<list id="headings">
						<record>
							<integer id="index" value="0" />
