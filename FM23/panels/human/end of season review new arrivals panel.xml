<panel>
	<attachment_group class="vertical_arrange_autosize" horizontal_alignment="horizontal" />
	<container class="main_box" entity_layout_id="at00" id="at00" name="background box">
		<attachment_group class="vertical_arrange_autosize" horizontal_alignment="horizontal" />
		<container class="object_portrait_picture" entity_layout_id="at01" id="tmPC" width="30" height="30" name="team icon">
			<string id="image_alignment" value="left" />
			<string id="scale_picture" value="true" />
			<string id="keep_aspect_ratio" value="true" />
		</container>
		<widget class="text" entity_layout_id="at02" id="ttlT" auto_size="horizontal" needs_width="true" height="30" name="the new arrivals" alignment="left,centre_y">
			<string id="font" value="title" />
			<string id="colour" value="text" />
			<string id="size" value="xxlarge" />
		</widget>
		<widget class="horizontal_solid_intro_divider" entity_layout_id="at03" id="ttlD" width="50" height="5">
			<string id="colour" value="fg" />
		</widget>
		<widget class="text" entity_layout_id="at04" id="stlT" auto_size="vertical" needs_height="true" name="It was a busy summer etc.">
			<string id="multiline" value="true" />
			<string id="colour" value="fg" />
			<string id="size" value="large" />
		</widget>
		<!--Tab Strip-->
		<widget class="tab_strip" entity_layout_id="at06" id="tabs" appearance="tabs/subsection" alignment="top" auto_size="horizontal" height="35" name="tabs" navigation_container="true" top_level_tab_focus="true">
			<string id="use_alt_end_tab_appearances" value="false" />
			<string id="select_event" value="etbS" />
			<list id="items">
				<record id="a_button" width="200">
					<string id="icon" value="icons/16px/transfer in" />
					<translation id="name" translation_id="491862" type="use" value="Transfers In[COMMENT: AREA: Confidence; SUB-AREA: Board Confidence; FR-BN: ?; TONE: Formal; CHARLIMIT: None; REF-ID: n/a; NOTE: title for summary overview of confidence regarding transfers into the club]" />
				</record>
				<record id="b_button" width="200">
					<string id="icon" value="icons/16px/transfer out" />
					<translation id="text" translation_id="491863" type="use" value="Transfers Out[COMMENT: AREA: Confidence; SUB-AREA: Board Confidence; FR-BN: ?; TONE: Formal; CHARLIMIT: None; REF-ID: n/a; NOTE: title for summary overview of confidence regarding transfers out of the club]" />
				</record>
				<record id="x_button" width="200">
					<string id="icon" value="icons/16px/transfer out" />
					<translation id="name" translation_id="516726" type="use" value="Loans Out[COMMENT: AREA: Confidence; SUB-AREA: Board Confidence; FR-BN: ?; TONE: Formal; CHARLIMIT: None; REF-ID: n/a; NOTE: title for summary overview of confidence regarding loan transfers out of the club]" />
				</record>
			</list>
			<string id="selection" value="0" />
		</widget>
		<!--Signing of Season Box-->
		<container class="horizontal_arrange_container" layout="300,-1" gap="10" auto_size="vertical">
			<container class="main_subtle_box" appearance="" entity_layout_id="at05" id="at05" name="signing of the season box" navigation_container="true">
				<attachment_group class="vertical_arrange_autosize" />
				<widget class="text" entity_layout_id="at09" id="tdlT" alignment="centre_x,centre_y" height="20" name="transfer details">
					<global id="hidden" name="ui.is_fmt_controller_skin"/>
					<translation id="text" value="Transfer Details" translation_id="247710" type="use" />
					<string id="font" value="title" />
					<string id="size" value="xlarge" />
					<string id="colour" value="text" />
				</widget>
				<container class="client_object_viewer_xml_panel" entity_layout_id="at08" file="widgets/player picture name and description widget vertical centered" id="vpFL" height="100" name="profile"/>

				<container class="horizontal_arrange_container" alignment="middle, extend" auto_size="vertical">
					<container class="client_object_property_panel" entity_layout_id="at0:" id="tsfM" appearance="boxes/custom/darken/rounded/no margin/paper" height="45" name="new team signed from widget">
						<string id="property" value="ptiT" />
						<string id="red_replacement" value="green a700" />

					</container>
					<container class="client_object_property_panel" entity_layout_id="at99" id="tsFM" appearance="boxes/custom/darken/rounded/no margin/paper" height="45"  name="new team signed from widget 2">
						<string id="property" value="ptoT" />
						<string id="red_replacement" value="red 500" />

					</container>
					<container class="client_object_property_panel" entity_layout_id="at98" id="tsOL" appearance="boxes/custom/darken/rounded/no margin/paper" height="45" name="new team signed from widget 3">
						<string id="property" value="ptoL" />
						<string id="red_replacement" value="red 500" />

					</container>
				</container>
				<container class="client_object_list_table" entity_layout_id="at0;" id="mstT" height="40" name="mini stats table">
					<global id="hidden" name="ui.is_fmt_controller_skin"/>
					<integer id="row_height" value="20" />
					<integer id="fixed_row_height" value="15" />
					<string id="customisation_context" value="play" />
					<string id="mode" value="printable, keyboard, fill_rows" />
					<integer id="database_table_type" value="1" />
					<boolean id="should_force_refresh" value="false" />
					<integer id="row_spacing" value="4" />
					<list id="default_sort_column" />
					<list id="column_widget_properties">
						<record>
							<integer id="index" value="0" />
							<string id="font" value="title" />
							<string id="size" value="large" />
							<string id="colour" value="text" />
						</record>
						<record>
							<integer id="index" value="1" />
							<string id="font" value="title" />
							<string id="size" value="large" />
							<string id="colour" value="text" />
						</record>
						<record>
							<integer id="index" value="2" />
							<string id="font" value="title" />
							<string id="size" value="large" />
							<string id="colour" value="text" />
						</record>
					</list>
					<record id="viewed_property_ids">
						<record id="player_overall_appearances" width="-4" />
						<record id="player_overall_goals" width="-3" />
						<record id="player_overall_assists" width="-3" auto_hide_priority="1" />
						<record id="player_overall_average_rating" width="-6" auto_hide_priority="1" />
					</record>

				</container>
				<widget class="horizontal_divider" entity_layout_id="at0&lt;" id="at0&lt;" height="5">
					<global id="hidden" name="ui.is_fmt_controller_skin"/>
				</widget>
				<container id="at0=" entity_layout_id="at0=" >
					<attachment_group class="vertical_arrange_autosize" horizontal_alignment="horizontal" />
					<widget class="label" id="at0&gt;" entity_layout_id="at0&gt;" auto_size="vertical" alignment="centre">
						<translation id="text" translation_id="423750" type="use" value="Board Transfer Reactions[COMMENT - team screen section containing board opinions on club transfer activity]" />
						<global id="hidden" name="ui.is_fmt_controller_skin"/>
						<string id="font" value="title" />
						<string id="size" value="medium" />
						<string id="colour" value="fg" />
					</widget>
					<widget class="performance_grade_box" id="trGB" entity_layout_id="at0?" auto_size="all">

					</widget>
				</container>
			</container>
			<!--Player Table-->
			<container class="client_object_list_table" entity_layout_id="at07" id="prsT" name="list table" height="700" navigation_focus_target="true">
				<global id="override_select_row_flags" name="platform_is_stadia"/>
				<integer id="row_height" value="70" />
				<integer id="fixed_row_height" value="30" />
				<string id="select_event" value="slTP" />
				<string id="customisation_context" value="play" />
				<string id="mode" value="printable, keyboard, fill_rows, dont_allow_empty_selection,  select_rows,  show_bullets, dont_ask_for_focus" />
				<integer id="database_table_type" value="1" />
				<boolean id="should_force_refresh" value="false" />
				<integer id="row_spacing" value="10" />
				<string id="row_appearance" value="boxes/custom/darken/rounded/no margin/paper" />
				<list id="default_sort_column">
					<string id="a_button" value="Poav" />
				</list>
				<list id="column_widget_properties">
					<record>
						<integer id="index" value="4" />
						<string id="style" value="bold" />
						<string id="size" value="medium" />
					</record>
					<record>
						<integer id="index" value="5" />
						<string id="style" value="bold" />
						<string id="size" value="medium" />
					</record>
					<record>
						<integer id="index" value="6" />
						<string id="style" value="bold" />
						<string id="size" value="medium" />
					</record>
				</list>
				<record id="viewed_property_ids">
					<record id="PeoS" width="265" auto_hide_priority="2" />
					<record id="ptiT" width="265" auto_hide_priority="2" />
					<record id="ptoT" width="265" auto_hide_priority="2" />
					<record id="ptoL" width="265" auto_hide_priority="2" />
					<record id="player_overall_appearances" width="50" auto_hide_priority="100" />
					<record id="player_overall_goals" width="50" auto_hide_priority="100" />
					<record id="player_overall_assists" width="50" auto_hide_priority="100" />
					<record id="player_overall_average_rating" width="50" auto_hide_priority="1" />
				</record>
			</container>
		</container>
	</container>
</panel>