			<layout class="stick_to_object_attachment" id="alPr" get_side="right" set_side="right" target="main"/>
		</widget>
		<widget class="person_button" colour="black" entity_layout_id="at02" id="pnmp" auto_size="vertical" needs_height="true">
			<string id="size" value="large"/>
			<string id="font" value="bold"/>
			<record id="dbrp">
				<string id="get_property" value="PnmP"/>
				<string id="set_property" value="valu"/>
			</record>
			<string id="colour" value="faded text"/>
			<layout class="stick_to_object_attachment" id="alPt" offset="4" get_side="bottom" set_side="top" move_side="true" target="at03"/>
			<layout class="stick_to_object_attachment" id="alPl" offset="4" get_side="left" set_side="left" target="main"/>
			<layout class="stick_to_object_attachment" id="alPr" get_side="right" set_side="right" target="main"/>
